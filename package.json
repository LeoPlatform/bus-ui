{
	"name": "botmon",
	"version": "2.1.0",
	"description": "",
	"main": "app.js",
	"scripts": {
		"start": "leo-cli test .",
		"old_start": "leo-cli test .",
		"test": "mocha --require ./test/bootstrap.js --recursive -w",
		"debug": "node-debug leo test ."
	},
	"config": {
		"leo": {
			"type": "microservice",
			"subtype": "react",
			"role": "ApiRole",
			"name": "Leo_Botmon",
			"display-name": "Bot Monitoring",
			"root": "botmon",
			"stacks": [
				"leosdk",
				"leoauth"
			],
			"aws": {
				"cognito_region": {
					"ref": "micro.cognito.region"
				},
				"cognito_id": {
					"ref": "micro.cognito.id"
				}
			},
			"micro": {
				"version": {
					"ref": "micro.version"
				}
			},
			"static": {
				"s3": {
					"ref": "micro.s3.id"
				},
				"cloudfront": {
					"ref": "micro.cloudfront.domain"
				}
			},
			"regions": [
				"us-west-2",
				"us-east-1"
			],
			"showPagesEnv": {
				"Resources": {
					"CognitoId": "${CognitoId}",
					"Region": "${AWS::Region}"
				}
			},
			"3rdParty": true,
			"excludeProfiles": true
		},
		"console": {
			"navigation": [
				{
					"title": "Botmon",
					"href": "/botmon",
					"icon": ""
				}
			]
		}
	},
	"author": "",
	"license": "ISC",
	"dependencies": {
		"babel-plugin-transform-decorators-legacy": "^1.3.4",
		"babel-preset-es2015": "^6.24.1",
		"babel-preset-react": "^6.24.1",
		"babel-preset-stage-0": "^6.24.1",
		"child_process": "^1.0.2",
		"csv-parse": "^1.1.7",
		"d3-jetpack": "^2.0.0",
		"deep-diff": "^1.0.0",
		"elasticsearch": "^12.1.3",
		"extend": "^3.0.1",
		"html5sortable": "^0.6.3",
		"install": "^0.8.1",
		"jsonpath": "^0.2.9",
		"later": "^1.2.0",
		"leo-auth": "^2.0.1",
		"leo-sdk": "^2.0.1",
		"material-ui": "^0.19.4",
		"mobx": "^3.3.0",
		"mobx-react": "^4.3.4",
		"mocha": "^3.0.2",
		"moment": "^2.13.0",
		"moment-round": "^1.0.1",
		"moment-timezone": "^0.5.13",
		"numeral": "^1.5.3",
		"q": "^1.4.1",
		"react": "^15.6.2",
		"react-autobind": "^1.0.6",
		"react-codemirror": "^1.0.0",
		"react-dom": "^15.6.2",
		"react-redux": "^4.4.5",
		"redux": "^3.6.0",
		"redux-thunk": "^2.1.0",
		"uuid": "^2.0.3"
	},
	"devDependencies": {
		"babel-preset-env": "^1.7.0"
	}
}
